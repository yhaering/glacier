{
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "transform": {
    "^.+\\.(t|j)sx?$": ["@swc/jest"]
  },
  "coverageDirectory": "coverage",
  "collectCoverage": true,
  "coverageReporters": ["clover", "json", "lcov", "text", "html"],
  "collectCoverageFrom": ["src/**/*.ts"]
}
